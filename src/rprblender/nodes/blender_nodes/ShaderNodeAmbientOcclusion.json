{
    "name": "ShaderNodeAmbientOcclusion",

    "inputs": {
        "color": {
            "type": "color",
            "label": "Color"
        },
        "inside": {
            "type": "int",
            "label": "inside"
        },
        "distance": {
            "type": "float",
            "label": "Distance"
        }
    },

    "outputs": {
        "Color": {
            "type": "color",
            "node": "mix"
        },
        "AO": {
            "type": "float",
            "node": "ao"
        }
    },

    "nodes": {
        "invert": {
            "name": "invert",
            "type": "RPR_MATERIAL_NODE_ARITHMETIC",
            "inputs": {
                "color0": "inputs.inside",
                "color1": [-1.0, 0.0, 0.0, 1.0],
                "op": "RPR_MATERIAL_NODE_OP_MUL"
            }
        },
        "ao": {
            "name": "ao",
            "type": "RPR_MATERIAL_NODE_AO_MAP",
            "inputs": {
                "radius": "inputs.distance",
                "side": "nodes.invert"
            }
        },
        "mix": {
            "name": "mix",
            "type": "RPR_MATERIAL_NODE_BLEND_VALUE",
            "inputs": {
                "color0": [0.0, 0.0, 0.0, 0.0],
                "color1": "inputs.color",
                "weight": "nodes.ao"
            }
        }
    }
}